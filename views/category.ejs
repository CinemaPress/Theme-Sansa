<!DOCTYPE html>
<html lang="<%- page.language %>">
<head>

    <% include includes/head.ejs %>

</head>
<body>

<% include includes/header.ejs %>

<div id="content">

    <div class="left">

        <% include includes/left.ejs %>

    </div>

    <div class="right">

        <div class="speedbar">
            <span id="cinemapress-speedbar">
                <a href="/"><%- page.l.home %></a> &#9658;
                <%= page.title %>
            </span>
        </div>

        <% if (typeof page.adv === 'object' && page.adv.top) { %>
        <div style="clear: both;"></div>
        <div style="text-align: center; margin: 10px auto">
            <%- page.adv.top %>
        </div>
        <div style="clear: both;"></div>
        <% } %>

        <div class="category-name_2">
            <h1><%- page.h1 %></h1>
        </div>

        <div style="clear: both;"></div>

        <% if (typeof content === 'object') { %>
        <div class="cinemapress-content">
            <% if (content.image) { %>
            <img src="<%- content.image %>" class="cinemapress-content-image">
            <% } %>
            <% if (content.description) { %>
            <div class="cinemapress-content-description">
                <%- content.description %>
            </div>
            <% } %>
            <div class="cinemapress-content-publish">
                <% if (content.tags) { %>
                <span class="cinemapress-content-publish-tags"><img src="data:image/svg+xml;utf8;base64," style="width: 10px;"> <%- content.tags %></span>
                <% } %>
                <% if (content.tags) { %>
                <span class="cinemapress-content-publish-publish"><img src="data:image/svg+xml;utf8;base64," style="width: 10px;"> <%- content.publish %></span>
                <% } %>
            </div>
        </div>
        <div style="clear: both;"></div>
        <% } %>

        <% if (typeof top === 'object' && top.length) { %>
        <div class="topcat">
            <div class="topcat_cont">
                <p class="tit"><%- page.l.top %></p>
                <div>
                    <ul class="hs_container" style="margin: 0;padding: 0;position: relative;list-style-type: none;z-index: 1;">
                        <% for (var m = 0; m < top.length; m++) { %>
                        <li>
                            <div class="tc_item">
                                <a href="<%- top[m].url %>" rel="nofollow">
                                    <img src="<%- top[m].poster %>" alt="<%= top[m].title %>" title="<%= top[m].title %>">
                                </a>
                                <p class="tc_item_title">
                                    <a href="<%- top[m].url %>"><%- top[m].title %></a>
                                </p>
                            </div>
                        </li>
                        <% if (m >= 4) break; } %>
                    </ul>
                </div>
            </div>
        </div>
        <% } %>

        <% if (typeof page.sorting === 'object') { %>
        <div class="sortn">
            <div>
                <% page.sorting.forEach(function (sort) {
                    if (sort.active) { %>
                <a href="<%- sort.url %>"> <strong><%- sort.name %></strong> </a>
                <% } else { %>
                <a href="<%- sort.url %>"> <%- sort.name %> </a>
                <% } %>
                <% }); %>
            </div>
        </div>
        <% } %>

        <div id="cinemapress-content">

            <% if (typeof movies === 'object' && movies.length) {
                movies.forEach(function (movie) { %>

            <div class="story story_m">
                <div class="s_name">
                    <div class="fname">
                        <a href="<%- movie.url %>">
                            <%= movie.title %>
                            <span><%- movie.year %></span> </a>
                    </div>
                </div>

                <% if (movie.rating) { %>

                <div class="rating show_rating_panel">
                    <span class="text"><%- page.l.rating %>:</span>
                    <div class="vote-num">
                        <span><%- movie.rating / 10 %></span>
                    </div>
                    <div class="clear"></div>
                </div>

                <% } %>

                <div class="poster">
                    <div class="poster_over_2">
                        <a href="<%- movie.url %>" rel="nofollow" class="poster_over_2_play"></a>
                        <% if (page.urls.movies.trailer) { %>
                        <a href="<%- movie.url %>/<%- page.urls.movies.trailer %>" rel="nofollow" class="poster_tr"></a>
                        <% } %>
                    </div>
                    <a href="<%- movie.url %>" rel="nofollow"><img src="<%- movie.poster %>" alt="<%= movie.title %>" title="<%= movie.title %>"> </a>
                </div>

                <div class="description">
                    <% if (movie.description_short) { %>
                    <p>
                        <span><%- page.l.storyline %>:</span>
                        <%- movie.description_short %>...
                    </p>
                    <% } %>
                    <div class="information">
                        <ul>
                            <% if (movie.year) { %>
                            <li><span><%- page.l.year %>:</span> <%- movie.year %></li>
                            <% } %>
                            <% if (movie.countries) { %>
                            <li><span><%- page.l.countries %>:</span> <%- movie.countries %></li>
                            <% } %>
                            <% if (movie.genres) { %>
                            <li><span><%- page.l.genres %>:</span> <%- movie.genres %></li>
                            <% } %>
                            <% if (movie.kp_vote) { %>
                            <li><span><%- page.l.rating %> <%- page.l.kp %>:</span> <%- movie.kp_rating / 10 %> (<%- movie.kp_vote %>)</li>
                            <% } %>
                            <% if (movie.imdb_vote) { %>
                            <li><span><%- page.l.rating %> <%- page.l.imdb %>:</span> <%- movie.imdb_rating / 10 %> (<%- movie.imdb_vote %>)</li>
                            <% } %>
                            <% if (movie.premiere) { %>
                            <li><span><%- page.l.premiere %>:</span> <%- movie.premiere %></li>
                            <% } %>
                            <% if (movie.quality) { %>
                            <li><span><%- page.l.quality %>:</span> <%- movie.quality %></li>
                            <% } %>
                            <% if (movie.translate) { %>
                            <li><span><%- page.l.translate %>:</span> <%- movie.translate %></li>
                            <% } else { %>
                            <li><br></li>
                            <% } %>
                        </ul>
                        <div class="read-more">
                            <a href="<%- movie.url %>" rel="nofollow" class="read"><span><%- page.l.more %> ...</span></a>
                        </div>
                    </div>
                </div>
            </div>

            <% }); } %>

            <% if (typeof page.pagination === 'object') { %>
            <div class="navigation">
                <span class="list-pages">
                    <% if (page.pagination.first) { %>
                    <a href="<%- page.pagination.first.link %>" rel="nofollow"><%- page.pagination.first.number %></a>
                    <span class="nav_ext">...</span>
                    <% } %>
                    <% if (page.pagination.prev.length) { %>
                    <% page.pagination.prev.forEach(function (prev) { %>
                    <a href="<%- prev.link %>" rel="nofollow"><%- prev.number %></a>
                    <% }); %>
                    <% } %>
                    <span><%- page.pagination.current %></span>
                    <% if (page.pagination.next.length) { %>
                    <% page.pagination.next.forEach(function (next) { %>
                    <a href="<%- next.link %>" rel="nofollow"><%- next.number %></a>
                    <% }); %>
                    <% } %>
                    <% if (page.pagination.last) { %>
                    <span class="nav_ext">...</span>
                    <a href="<%- page.pagination.last.link %>" rel="nofollow"><%- page.pagination.last.number %></a>
                    <% } %>
                </span>
            </div>
            <% } %>

            <% if (typeof content === 'object' && page.comments) { %>
            <div style="padding: 10px;">
                <%- page.comments %>
            </div>
            <% } %>

        </div>

        <% if (typeof page.adv === 'object' && page.adv.bottom) { %>
        <div style="clear: both;"></div>
        <div style="text-align: center; margin: 10px auto">
            <%- page.adv.bottom %>
        </div>
        <div style="clear: both;"></div>
        <% } %>

        <% if (page.seo) { %>
        <div style="clear: both;"></div>
        <div class="main-info">
            <div class="page_seo">
                <%- page.seo %>
            </div>
        </div>
        <div style="clear: both;"></div>
        <% } %>

        <div class="main-info recentlyViewedBlock" style="margin: 20px 0 0 0; display: none;">
            <div><%- page.l.watched %>:</div>
            <div id="recentlyViewed"></div>
        </div>

    </div>

    <div style="clear: both;"></div>

</div>

<div id="footer">

    <% include includes/footer.ejs %>

</div>

</body>
</html>